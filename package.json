{
  "name": "linearctl",
  "description": "Linear CLI - Developer-friendly Linear API client",
  "version": "0.1.3",
  "author": "nacyot",
  "bin": {
    "linearctl": "./bin/run.js",
    "lc": "./bin/run.js"
  },
  "bugs": "https://github.com/nacyot/linearctl/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.4",
    "@linear/sdk": "^59.1.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.6.2",
    "conf": "^14.0.0",
    "dotenv": "^17.2.2",
    "string-width": "^8.1.0",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@vitest/ui": "^3.2.4",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./docs",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nacyot/linearctl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "lc",
    "dirname": "lc",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "issue": {
        "description": "Manage Linear issues"
      },
      "comment": {
        "description": "Manage comments on issues"
      },
      "team": {
        "description": "View teams"
      },
      "user": {
        "description": "View users"
      },
      "project": {
        "description": "Manage projects"
      },
      "rule": {
        "description": "Manage Linear CLI rules and guides"
      }
    }
  },
  "repository": "nacyot/linearctl",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "types": "dist/index.d.ts"
}
